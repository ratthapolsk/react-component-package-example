{
  "name": "@ratthapolsk/react-component-package-example",
  "version": "0.0.1",
  "private": false,
  "description": "React Component Package Example",
  "author": "Ratthapol Sakonchaiphraisan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratthapolsk/react-component-package-example.git"
  },
  "bugs": {
    "url": "https://github.com/ratthapolsk/react-component-package-example/issues"
  },
  "homepage": "https://github.com/ratthapolsk/react-component-package-example#readme",
  "keywords": [
    "React",
    "React Component",
    "Example Repository"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "> 7%"
  ],
  "publishConfig": {
    "access": "public",
    "branches": [
      "main"
    ]
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "pnpm run clean && tsup src/index.ts --format cjs,esm --dts",
    "clean": "rimraf dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0"
  }
}
